<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>

<p>
  <b>Role:</b>
  <%= @project.role %>
</p>

<p class="description">
  <b>Description:</b>
  <%= @project.description %>
</p>


<% for image in @project.images %>
	<%= image_tag image.image_url.to_s, :class => 'image' %>
<% end %>

<% if user_signed_in? %>
<%= link_to 'Edit', edit_project_path(@project) %> |
<% end %>
<%= link_to 'Back', projects_path %>
